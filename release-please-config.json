{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "simple",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "pull-request-title-pattern": "chore: experiment{scope} release {version}",
      "pull-request-header": "This is a custom message for PR",
      "pull-request-footer": "Approve in circle CI to deploy",
      "draft": false,
      "prerelease": false
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}